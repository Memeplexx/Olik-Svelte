{
  "name": "olik-svelte",
  "version": "0.1.2",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": true,
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js && rimraf out-tsc",
    "dev": "rollup -c --watch",
    "test": "cross-env NODE_ENV=test jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:cov": "cross-env NODE_ENV=test npm test --coverage --collectCoverageFrom=src/**/*.{ts}",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --verbose false"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@types/jest": "^26.0.7",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "eslint": "^7.31.0",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.1.4",
    "typescript": "4.0.3",
    "olik": "^0.1.42"
  },
  "peerDependencies": {
    "olik": "^0.1.42"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
